{
  "name": "@proluofire/proluofire-im",
  "version": "2026.1.29",
  "description": "OpenClaw proluofire-im channel plugin",
  "type": "module",
  "dependencies": {
    "ws": "^8.19.0",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "openclaw": "workspace:*"
  },
  "openclaw": {
    "extensions": [
      "./index.ts"
    ],
    "channel": {
      "id": "proluofire-im",
      "label": "Proluofire IM",
      "selectionLabel": "Proluofire IM (plugin)",
      "docsPath": "/channels/proluofire-im",
      "docsLabel": "proluofire-im",
      "blurb": "custom IM system; install the plugin to enable.",
      "order": 80,
      "quickstartAllowFrom": true
    },
    "install": {
      "npmSpec": "@proluofire/proluofire-im",
      "localPath": "extensions/proluofire-im",
      "defaultChoice": "npm"
    }
  }
}
